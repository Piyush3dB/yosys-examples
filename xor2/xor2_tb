#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x7fffd4cb5100 .scope module, "main" "main" 2 3;
 .timescale -9 -9;
v0x7fffd4cc8830_0 .var "a", 0 0;
v0x7fffd4cc8900_0 .var "b", 0 0;
v0x7fffd4cc89d0_0 .net "f", 0 0, L_0x7fffd4cc8c60;  1 drivers
v0x7fffd4cc8ad0_0 .var/i "idx", 31 0;
v0x7fffd4cc8b70_0 .var "inputs", 1 0;
S_0x7fffd4cb5290 .scope module, "test" "xor2" 2 9, 3 1 0, S_0x7fffd4cb5100;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "f";
    .port_info 1 /INPUT 1 "a";
    .port_info 2 /INPUT 1 "b";
L_0x7fffd4cc8c60 .functor XOR 1, v0x7fffd4cc8830_0, v0x7fffd4cc8900_0, C4<0>, C4<0>;
v0x7fffd4ca3da0_0 .net "a", 0 0, v0x7fffd4cc8830_0;  1 drivers
v0x7fffd4cc8650_0 .net "b", 0 0, v0x7fffd4cc8900_0;  1 drivers
v0x7fffd4cc8710_0 .net "f", 0 0, L_0x7fffd4cc8c60;  alias, 1 drivers
    .scope S_0x7fffd4cb5100;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffd4cc8ad0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7fffd4cc8ad0_0;
    %cmpi/s 3, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x7fffd4cc8ad0_0;
    %pad/s 2;
    %store/vec4 v0x7fffd4cc8b70_0, 0, 2;
    %load/vec4 v0x7fffd4cc8b70_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x7fffd4cc8830_0, 0, 1;
    %load/vec4 v0x7fffd4cc8b70_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x7fffd4cc8900_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 21 "$display", "%t: a=%b, b=%b, f=%b", $time, v0x7fffd4cc8830_0, v0x7fffd4cc8900_0, v0x7fffd4cc89d0_0 {0 0 0};
    %load/vec4 v0x7fffd4cc8ad0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffd4cc8ad0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
