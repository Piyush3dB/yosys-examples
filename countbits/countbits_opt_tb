#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x7fffd04e3f60 .scope module, "countbits_tb" "countbits_tb" 2 3;
 .timescale -9 -9;
P_0x7fffd04cd460 .param/l "CLK_PERIOD" 0 2 5, +C4<00000000000000000000000000001010>;
P_0x7fffd04cd4a0 .param/l "DATA_WIDTH" 0 2 4, +C4<00000000000000000000000000000100>;
L_0x7f37c2070018 .functor BUFT 1, C4<0000000000000000000000000000>, C4<0>, C4<0>, C4<0>;
v0x7fffd04fae40_0 .net *"_ivl_3", 27 0, L_0x7f37c2070018;  1 drivers
v0x7fffd04faf20_0 .var "clk", 0 0;
v0x7fffd04fafe0_0 .var "data", 3 0;
v0x7fffd04fb080_0 .var/i "i", 31 0;
v0x7fffd04fb140_0 .net "ones", 2 0, v0x7fffd04fabb0_0;  1 drivers
L_0x7fffd04fb250 .concat [ 4 28 0 0], v0x7fffd04fafe0_0, L_0x7f37c2070018;
S_0x7fffd04e41c0 .scope module, "uut" "countbits" 2 11, 3 3 0, S_0x7fffd04e3f60;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 32 "data";
    .port_info 2 /OUTPUT 3 "ones";
P_0x7fffd04e44a0 .param/l "DATA_WIDTH" 0 3 5, +C4<00000000000000000000000000100000>;
v0x7fffd04d2b60_0 .net "clk", 0 0, v0x7fffd04faf20_0;  1 drivers
v0x7fffd04faa10_0 .net "data", 31 0, L_0x7fffd04fb250;  1 drivers
v0x7fffd04faaf0_0 .var/i "i", 31 0;
v0x7fffd04fabb0_0 .var "ones", 2 0;
v0x7fffd04fac90_0 .var "v_count", 2 0;
E_0x7fffd04e27c0 .event posedge, v0x7fffd04d2b60_0;
S_0x7fffd04d2980 .scope begin, "COUNT_BITS" "COUNT_BITS" 3 14, 3 14 0, S_0x7fffd04e41c0;
 .timescale 0 0;
    .scope S_0x7fffd04e41c0;
T_0 ;
    %wait E_0x7fffd04e27c0;
    %fork t_1, S_0x7fffd04d2980;
    %jmp t_0;
    .scope S_0x7fffd04d2980;
t_1 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x7fffd04fac90_0, 0, 3;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffd04faaf0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x7fffd04faaf0_0;
    %cmpi/s 31, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x7fffd04fac90_0;
    %load/vec4 v0x7fffd04faa10_0;
    %load/vec4 v0x7fffd04faaf0_0;
    %part/s 1;
    %pad/u 3;
    %add;
    %store/vec4 v0x7fffd04fac90_0, 0, 3;
    %load/vec4 v0x7fffd04faaf0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffd04faaf0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x7fffd04fac90_0;
    %assign/vec4 v0x7fffd04fabb0_0, 0;
    %end;
    .scope S_0x7fffd04e41c0;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fffd04e3f60;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffd04faf20_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x7fffd04e3f60;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x7fffd04faf20_0;
    %inv;
    %store/vec4 v0x7fffd04faf20_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x7fffd04e3f60;
T_3 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffd04fb080_0, 0, 32;
T_3.0 ;
    %load/vec4 v0x7fffd04fb080_0;
    %cmpi/s 15, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_3.1, 5;
    %load/vec4 v0x7fffd04fb080_0;
    %pad/s 4;
    %store/vec4 v0x7fffd04fafe0_0, 0, 4;
    %delay 10, 0;
    %vpi_call 2 36 "$display", "%t: data=%h, ones=%d", $time, v0x7fffd04fafe0_0, v0x7fffd04fb140_0 {0 0 0};
    %load/vec4 v0x7fffd04fb080_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffd04fb080_0, 0, 32;
    %jmp T_3.0;
T_3.1 ;
    %delay 10, 0;
    %vpi_call 2 40 "$display", "End simulation time reached." {0 0 0};
    %vpi_call 2 41 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x7fffd04e3f60;
T_4 ;
    %vpi_call 2 47 "$dumpfile", "countbits.vcd" {0 0 0};
    %vpi_call 2 48 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffd04e3f60 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "countbits_tb.v";
    "countbits_opt.v";
