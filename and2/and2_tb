#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x8613838 .scope module, "main" "main" 2 3;
 .timescale -9 -9;
v0x8610950_0 .var "a", 0 0;
v0x86109d0_0 .var "b", 0 0;
v0x8610a40_0 .net "f", 0 0, L_0x8610b68; 1 drivers
v0x8610ab0_0 .var/i "idx", 31 0;
v0x8610b18_0 .var "inputs", 1 0;
S_0x85f09a8 .scope module, "test" "and2" 2 9, 3 1, S_0x8613838;
 .timescale 0 0;
L_0x8610b68 .functor AND 1, v0x8610950_0, v0x86109d0_0, C4<1>, C4<1>;
v0x85f0a28_0 .net "a", 0 0, v0x8610950_0; 1 drivers
v0x8610890_0 .net "b", 0 0, v0x86109d0_0; 1 drivers
v0x86108f0_0 .alias "f", 0 0, v0x8610a40_0;
    .scope S_0x8613838;
T_0 ;
    %set/v v0x8610ab0_0, 0, 32;
T_0.0 ;
    %load/v 8, v0x8610ab0_0, 32;
   %cmpi/s 8, 3, 32;
    %or 5, 4, 1;
    %jmp/0xz T_0.1, 5;
    %load/v 8, v0x8610ab0_0, 32;
    %set/v v0x8610b18_0, 8, 2;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.2, 4;
    %load/x1p 8, v0x8610b18_0, 1;
    %jmp T_0.3;
T_0.2 ;
    %mov 8, 2, 1;
T_0.3 ;
; Save base=8 wid=1 in lookaside.
    %set/v v0x8610950_0, 8, 1;
    %load/v 8, v0x8610b18_0, 1; Only need 1 of 2 bits
; Save base=8 wid=1 in lookaside.
    %set/v v0x86109d0_0, 8, 1;
    %delay 10, 0;
    %vpi_call 2 21 "$display", "%t: a=%b, b=%b, f=%b", $time, v0x8610950_0, v0x86109d0_0, v0x8610a40_0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x8610ab0_0, 32;
    %set/v v0x8610ab0_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "and2_tb.v";
    "and2.v";
