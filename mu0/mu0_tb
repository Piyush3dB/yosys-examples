v0x7fffc084a340_0 .var "pc", 15 0;
v0x7fffc084a420_0 .net "reset", 0 0, v0x7fffc084a900_0;  1 drivers
E_0x7fffc082bbf0/0 .event edge, v0x7fffc082aa80_0, v0x7fffc084a340_0, v0x7fffc0833d40_0, v0x7fffc082a780_0;
E_0x7fffc082bbf0/1 .event edge, v0x7fffc084a260_0;
E_0x7fffc082bbf0 .event/or E_0x7fffc082bbf0/0, E_0x7fffc082bbf0/1;
E_0x7fffc082b200 .event posedge, v0x7fffc082a900_0;
    .scope S_0x7fffc08328a0;
T_0 ;
