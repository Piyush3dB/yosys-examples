#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x7fffe8d69100 .scope module, "main" "main" 2 3;
 .timescale -9 -11;
v0x7fffe8d84b20_0 .var "addr", 3 0;
v0x7fffe8d84c00_0 .var "clk", 0 0;
v0x7fffe8d84cd0_0 .net "data", 7 0, v0x7fffe8d848b0_0;  1 drivers
v0x7fffe8d84dd0_0 .var/i "idx", 31 0;
v0x7fffe8d84e70_0 .var "re", 0 0;
S_0x7fffe8d69290 .scope module, "uut" "rom16x8" 2 9, 3 1 0, S_0x7fffe8d69100;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "re";
    .port_info 2 /INPUT 4 "addr";
    .port_info 3 /OUTPUT 8 "data";
v0x7fffe8d437d0 .array "ROM", 0 15, 7 0;
v0x7fffe8d5b060_0 .net "addr", 3 0, v0x7fffe8d84b20_0;  1 drivers
v0x7fffe8d847e0_0 .net "clk", 0 0, v0x7fffe8d84c00_0;  1 drivers
v0x7fffe8d848b0_0 .var "data", 7 0;
v0x7fffe8d84990_0 .net "re", 0 0, v0x7fffe8d84e70_0;  1 drivers
E_0x7fffe8d6e840 .event posedge, v0x7fffe8d847e0_0;
    .scope S_0x7fffe8d69290;
T_0 ;
    %pushi/vec4 1, 0, 8;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 2, 0, 8;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 4, 0, 8;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 8, 0, 8;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 16, 0, 8;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 32, 0, 8;
    %ix/load 4, 5, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 64, 0, 8;
    %ix/load 4, 6, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 128, 0, 8;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 1, 0, 8;
    %ix/load 4, 8, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 3, 0, 8;
    %ix/load 4, 9, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 7, 0, 8;
    %ix/load 4, 10, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 15, 0, 8;
    %ix/load 4, 11, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 31, 0, 8;
    %ix/load 4, 12, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 63, 0, 8;
    %ix/load 4, 13, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 127, 0, 8;
    %ix/load 4, 14, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %pushi/vec4 255, 0, 8;
    %ix/load 4, 15, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x7fffe8d437d0, 4, 0;
    %end;
    .thread T_0;
    .scope S_0x7fffe8d69290;
T_1 ;
    %wait E_0x7fffe8d6e840;
    %load/vec4 v0x7fffe8d84990_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x7fffe8d5b060_0;
    %pad/u 6;
    %ix/vec4 4;
    %load/vec4a v0x7fffe8d437d0, 4;
    %assign/vec4 v0x7fffe8d848b0_0, 0;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x7fffe8d69100;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffe8d84c00_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x7fffe8d69100;
T_3 ;
    %delay 2500, 0;
    %load/vec4 v0x7fffe8d84c00_0;
    %inv;
    %store/vec4 v0x7fffe8d84c00_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x7fffe8d69100;
T_4 ;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffe8d84e70_0, 0, 1;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x7fffe8d84b20_0, 0, 4;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fffe8d84dd0_0, 0, 32;
T_4.0 ;
    %load/vec4 v0x7fffe8d84dd0_0;
    %cmpi/s 15, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_4.1, 5;
    %delay 5000, 0;
    %load/vec4 v0x7fffe8d84dd0_0;
    %pad/s 4;
    %store/vec4 v0x7fffe8d84b20_0, 0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffe8d84e70_0, 0, 1;
    %load/vec4 v0x7fffe8d84dd0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x7fffe8d84dd0_0, 0, 32;
    %jmp T_4.0;
T_4.1 ;
    %delay 5000, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x7fffe8d69100;
T_5 ;
    %vpi_call 2 35 "$monitor", "%t: clk = %b, re=%b, addr=%h, data=%h", $time, v0x7fffe8d84c00_0, v0x7fffe8d84e70_0, v0x7fffe8d84b20_0, v0x7fffe8d84cd0_0 {0 0 0};
    %end;
    .thread T_5;
    .scope S_0x7fffe8d69100;
T_6 ;
    %vpi_call 2 41 "$dumpfile", "rom16x8.vcd" {0 0 0};
    %vpi_call 2 44 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fffe8d69100 {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 4;
