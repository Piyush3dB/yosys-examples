:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x7ffff77f7100 .scope module, "main" "main" 2 3;
 .timescale -9 -9;
v0x7ffff780ec80_0 .var "clk", 0 0;
v0x7ffff780ed40_0 .var "din", 7 0;
v0x7ffff780ee10_0 .net "dout", 7 0, v0x7ffff780e920_0;  1 drivers
v0x7ffff780ef10_0 .var "ld", 0 0;
v0x7ffff780efe0_0 .var "reset", 0 0;
S_0x7ffff77f7290 .scope module, "test" "register8" 2 8, 3 1 0, S_0x7ffff77f7100;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "ld";
    .port_info 3 /INPUT 8 "din";
    .port_info 4 /OUTPUT 8 "dout";
v0x7ffff77e70d0_0 .net "clk", 0 0, v0x7ffff780ec80_0;  1 drivers
v0x7ffff780e840_0 .net "din", 7 0, v0x7ffff780ed40_0;  1 drivers
v0x7ffff780e920_0 .var "dout", 7 0;
v0x7ffff780ea10_0 .net "ld", 0 0, v0x7ffff780ef10_0;  1 drivers
v0x7ffff780ead0_0 .net "reset", 0 0, v0x7ffff780efe0_0;  1 drivers
E_0x7ffff77f96e0 .event posedge, v0x7ffff780ead0_0, v0x7ffff77e70d0_0;
    .scope S_0x7ffff77f7290;
T_0 ;
    %wait E_0x7ffff77f96e0;
    %load/vec4 v0x7ffff780ead0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x7ffff780e920_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7ffff780ea10_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x7ffff780e840_0;
    %assign/vec4 v0x7ffff780e920_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7ffff77f7100;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ffff780ec80_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x7ffff77f7100;
T_2 ;
    %delay 25, 0;
    %load/vec4 v0x7ffff780ec80_0;
    %inv;
    %store/vec4 v0x7ffff780ec80_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x7ffff77f7100;
T_3 ;
    %delay 10, 0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x7ffff780ed40_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ffff780ef10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7ffff780efe0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x7ffff780ed40_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7ffff780ef10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ffff780efe0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x7ffff780ed40_0, 0, 8;
    %delay 50, 0;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x7ffff780ed40_0, 0, 8;
    %delay 50, 0;
    %pushi/vec4 85, 0, 8;
    %store/vec4 v0x7ffff780ed40_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7ffff780ef10_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x7ffff780ed40_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7ffff780ef10_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 186, 0, 8;
    %store/vec4 v0x7ffff780ed40_0, 0, 8;
    %delay 50, 0;
    %pushi/vec4 11, 0, 8;
    %store/vec4 v0x7ffff780ed40_0, 0, 8;
    %delay 50, 0;
    %pushi/vec4 171, 0, 8;
    %store/vec4 v0x7ffff780ed40_0, 0, 8;
    %delay 50, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
