#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x884ef48 .scope module, "main" "main" 2 3;
 .timescale -9 -9;
v0x886f008_0 .var "a", 0 0;
v0x886f088_0 .var "b", 0 0;
v0x886f0f8_0 .net "f", 0 0, v0x886ef40_0; 1 drivers
v0x886f168_0 .var/i "idx", 31 0;
v0x886f1d0_0 .var "inputs", 2 0;
v0x886f220_0 .var "sel", 0 0;
S_0x884efc8 .scope module, "test" "mux2to1" 2 9, 3 1, S_0x884ef48;
 .timescale 0 0;
v0x884f048_0 .net "a", 0 0, v0x886f008_0; 1 drivers
v0x886eee0_0 .net "b", 0 0, v0x886f088_0; 1 drivers
v0x886ef40_0 .var "f", 0 0;
v0x886efa0_0 .net "sel", 0 0, v0x886f220_0; 1 drivers
E_0x8871628 .event edge, v0x886efa0_0, v0x886eee0_0, v0x884f048_0;
    .scope S_0x884efc8;
T_0 ;
    %wait E_0x8871628;
    %load/v 8, v0x886efa0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %load/v 8, v0x886eee0_0, 1;
    %set/v v0x886ef40_0, 8, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x884f048_0, 1;
    %set/v v0x886ef40_0, 8, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x884ef48;
T_1 ;
    %set/v v0x886f168_0, 0, 32;
T_1.0 ;
    %load/v 8, v0x886f168_0, 32;
   %cmpi/s 8, 7, 32;
    %or 5, 4, 1;
    %jmp/0xz T_1.1, 5;
    %load/v 8, v0x886f168_0, 32;
    %set/v v0x886f1d0_0, 8, 3;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.2, 4;
    %load/x1p 8, v0x886f1d0_0, 1;
    %jmp T_1.3;
T_1.2 ;
    %mov 8, 2, 1;
T_1.3 ;
; Save base=8 wid=1 in lookaside.
    %set/v v0x886f220_0, 8, 1;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_1.4, 4;
    %load/x1p 8, v0x886f1d0_0, 1;
    %jmp T_1.5;
T_1.4 ;
    %mov 8, 2, 1;
T_1.5 ;
; Save base=8 wid=1 in lookaside.
    %set/v v0x886f008_0, 8, 1;
    %load/v 8, v0x886f1d0_0, 1; Only need 1 of 3 bits
; Save base=8 wid=1 in lookaside.
    %set/v v0x886f088_0, 8, 1;
    %delay 10, 0;
    %vpi_call 2 22 "$display", "%t: sel=%b, a=%b, b=%b, f=%b", $time, v0x886f220_0, v0x886f008_0, v0x886f088_0, v0x886f0f8_0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x886f168_0, 32;
    %set/v v0x886f168_0, 8, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "mux2to1_tb.v";
    "mux2to1.v";
