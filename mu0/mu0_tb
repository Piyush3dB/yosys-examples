    %cmp/u;
    %jmp/1 T_2.6, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 4;
    %cmp/u;
    %jmp/1 T_2.7, 6;
    %jmp T_2.9;
T_2.0 ;
    %load/vec4 v0x7fffc082a780_0;
    %pad/u 14;
    %ix/vec4 4;
    %load/vec4a v0x7fffc0832dc0, 4;
