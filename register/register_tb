    .scope S_0x7ffff77f7100;
T_4 ;
    %vpi_call 2 35 "$monitor", "%t: reset=%b, ld=%b, din=%b, dout=%b", $time, v0x7ffff780efe0_0, v0x7ffff780ef10_0, v0x7ffff780ed40_0, v0x7ffff780ee10_0 {0 0 0};
    %end;
    .thread T_4;
