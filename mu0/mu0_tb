    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x7fffc0833d40_0, 0;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x7fffc084a340_0, 0;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x7fffc082aa80_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %ix/getv 4, v0x7fffc084a340_0;
    %load/vec4a v0x7fffc0832dc0, 4;
    %store/vec4 v0x7fffc0833d40_0, 0, 16;
    %load/vec4 v0x7fffc0833d40_0;
    %parti/s 4, 12, 5;
    %cmpi/ne 7, 0, 4;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x7fffc084a340_0;
    %addi 1, 0, 16;
    %store/vec4 v0x7fffc084a340_0, 0, 16;
T_1.2 ;
T_1.1 ;
